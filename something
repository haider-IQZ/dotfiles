sudo rm /etc/machine-id /var/lib/dbus/machine-id && sudo dbus-uuidgen --ensure=/etc/machine-id && sudo dbus-uuidgen --ensure=/var/lib/dbus/machine-id && sudo systemctl restart dbus && sudo systemctl daemon-reexec






# Change machine-id
sudo rm /etc/machine-id /var/lib/dbus/machine-id
sudo dbus-uuidgen --ensure=/etc/machine-id
sudo dbus-uuidgen --ensure=/var/lib/dbus/machine-id

# Then REBOOT (don't restart services!)
sudo reboot




